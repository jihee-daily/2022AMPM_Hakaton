        android:supportsRtl="true"
        android:theme="@style/Theme.Mooda"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".pictureBig"
            android:exported="true" />
        <activity
            android:name="kr.ac.jbnu.se.sentiment.diary.eachSelect"
            android:exported="false" />
        <activity
            android:name="kr.ac.jbnu.se.sentiment.diary.write"
            android:exported="false" />
        <activity
            android:name="kr.ac.jbnu.se.sentiment.diary.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>